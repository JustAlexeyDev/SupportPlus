# Frontend Environment Variables
# Copy this file to .env and fill in your values
# Note: React requires REACT_APP_ prefix for custom variables

# API Configuration
# Leave empty for auto-detection based on hostname
# Or specify explicit URL: http://192.168.43.204:8000
REACT_APP_API_URL=

# Development Server Configuration
# These can be set via environment variables when running npm run dev
# HOST=0.0.0.0
# PORT=3000
